{
    "file_type": "json",
    "name": "words",
    "transducers": [{
            "input_field": "word",
            "output_field": "compare_form",
            "functions": [
                "./transducers/danish-approx.yaml"
            ]
        },
        {
            "input_field": "word",
            "output_field": "sort_form",
            "functions": [
                "lambda x: x.lower()"
            ]
        }
    ],
    "targets": {
        "entryID": "entryID",
        "definition": "definition",
        "word": "word",
        "audio": [{
            "description": "speaker",
            "filename": "filename"
        }],
        "theme": "theme",
        "optional": {
            "Part of Speech": "optional.\"Part of Speech\""
        }
    }
}
